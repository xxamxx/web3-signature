{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "pretty": false,
    "declaration": true,
    "paths": {},
    "outDir": "./dist"
  },
  "exclude": [
    "test"
  ]
}